{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "plugins": [],
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi": 2,
    "space-before-function-paren": 2
  }
}
